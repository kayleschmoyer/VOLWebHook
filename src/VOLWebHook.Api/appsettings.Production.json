{
  "Webhook": {
    "PayloadStoragePath": "/var/data/volwebhook/webhooks",
    "PayloadRetentionDays": 90
  },
  "Security": {
    "IpAllowlist": {
      "Enabled": false,
      "AllowPrivateNetworks": false
    },
    "RateLimit": {
      "Enabled": true,
      "RequestsPerMinute": 60,
      "RequestsPerHour": 500
    }
  },
  "FileLogging": {
    "LogDirectory": "/var/log/volwebhook"
  }
}
